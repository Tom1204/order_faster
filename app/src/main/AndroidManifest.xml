<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iut.finalproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".client.ui.activities.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".client.ui.activities.FoodActivity" />
        <activity android:name=".client.ui.activities.Register" />
        <activity android:name=".client.ui.activities.ItemDetailActivity" />
        <activity android:name=".client.ui.activities.CartActivity" />
        <activity android:name=".manage.ui.activities.OrderItemActivity"></activity>
    </application>

</manifest>